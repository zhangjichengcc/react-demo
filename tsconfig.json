{
  "compilerOptions": {
    "outDir": "./dist/",
    "sourceMap": true,
    "baseUrl": ".",
    "noImplicitAny": true,
    "module": "commonjs",
    "target": "es5",
    "allowJs": true,
    "moduleResolution": "node",
    "paths": {
      "@/*": ["./src/*"],
      "@data/*": ["./src/data/*"],
      "@templates/*": ["./src/templates/*"],
    }
  }
}
